<form action="" method="get" class="form-horizontal">
  <div class="form-group form-group-lg">

    <label  class="col-sm-2 control-label" for="formGroupInputLarge"  for="id">
      <input type="checkbox" name="cid" value="true"<%= query.cid ? 'checked' : '' %>>
      ID</label>
      <div class="col-sm-10">
        <input id="id" type="text" name="id" value="<%= query.id %>" class="form-control" placeholder="id"><br>
      </div>

      <label  class="col-sm-2 control-label" for="formGroupInputLarge">
        <input type="checkbox" name="cstring" value="true"<%= query.sctring ? 'checked' : '' %>>
        String</label>
        <div class="col-sm-10">
          <input id="string" type="text" name="string" value="<%= query.string%>" class="form-control" placeholder="String"><br>
        </div>

        <label class="col-sm-2 control-label" for="formGroupInputLarge">
          <input type="checkbox" name="cinteger" value="true"<%= query.cinteger ? 'checked' : '' %>>
          Integer</label>
          <div class="col-sm-10">
            <input id="number" type="number" name="integer" value="<%= query.integer%>" class="form-control" placeholder="Integer"><br>
          </div>

          <label  class="col-sm-2 control-label" for="formGroupInputLarge">
            <input type="checkbox" name="cfloat" value="true"<%= query.cfloat ? 'checked' : '' %>>
            Float</label>
            <div class="col-sm-10">
              <input id="float" type="float" name="float"  value="<%= query.float%>" class="form-control" placeholder="Float"><br>
            </div>

            <label  class="col-sm-2 control-label" for="formGroupInputLarge">
              <input type="checkbox" name="cdate" value="true"<%= query.cdate ? 'checked' : '' %>>
              Date</label>
              <div class="col-sm-10">
                <div class="col-sm-6">
                  <input type="date" class="form-control" name="startdate" value="<%= query.startdate %>">
                  To
                  <input type="date" class="form-control" name="enddate" value="<%= query.enddate %>">
                </div>
              </div>
              <br>
              <label  class="col-sm-2 control-label" for="formGroupInputLarge">
                <input type="checkbox" name="cboolean" value="true"<%= query.cboolean ? 'checked' : '' %>>
                Boolean</label>
                <div class="col-sm-10">
                  <select name="boolean" class="form-control" >

                    <option value="true"<%= query.boolean == "true" ? ' selected' : '' %>>True</option>
                    <option value="false"<%= query.boolean == "false" ? ' selected' : '' %>>False</option>

                  </select>
                </div>
                <br><br>


                <label  class="col-sm-2 control-label" for="formGroupInputLarge"></label>
                <div class="col-sm-offset-2 col-sm-10">
                  <button  style="margin-top : 30px"type="submit" name="search" class="btn btn-primary"> Search </button> <br>
                </div>
              </div>
            </div>
          </form>
        </div>
